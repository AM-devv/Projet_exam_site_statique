\n$font-2: 'Source Sans Pro', sans-serif;\n$font-color-h1: $color-text;\n$font-size-h1: 2.5rem;\n$font-weight-h1: 900;\n$line-height-h1: 1.2;\n$margin-h1: 0 0 .7rem 0;\n\n$font-size-h2: 1.5rem;\n$font-weight-h2: 900;\n$line-height-h2: 1.5;\n$margin-h2: 0 0 .7rem 0;\n\n$font-size-h3: 1.25rem;\n$font-weight-h3: 900;\n$line-height-h3: 1.5;\n$margin-h3: 0 0 .7rem 0;\n\n$font-size-h4: 1.1rem;\n$font-weight-h4: 900;\n$line-height-h4: 1.2;\n$margin-h4: 0 0 .7rem 0;\n\n// LINKS\n$link-color: $color-text;\n$link-decoration: underline dotted;\n$link-hover-color: $color-1;\n$link-hover-decoration: none;\n\n// CODE\n$code-color: $color-text;\n$code-background: $color-2;\n$code-padding: .3em;\n$code-border: $color-3 1px solid;\n$code-border-radius: $border-radius-default;\n\n// HR\n$hr-height: 1px;\n$hr-color: $color-3;\n$hr-margin: 3rem 0;\n\n//--------------------------\n// DEFAULT SPECIFICATIONS\n//--------------------------\nbody{\n    font-family: $font-1;\n    font-size: $font-size-default;\n    font-weight: $font-weight-default;\n    line-height: $line-height-default;\n    color: $font-color-default;\n}\n\na{\n    color: $link-color;\n    text-decoration: $link-decoration;\n    &:hover{\n        color: $link-hover-color;\n        text-decoration: $link-hover-decoration;\n    }\n}\n\nhr{\n    height: $hr-height;\n    background: $hr-color;\n    margin: $hr-margin;\n    border: none;\n}\n\ncode{\n    color: $code-color;\n    background: $code-background;\n    padding: $code-border;\n    border: $code-border;\n    border-radius: $code-border-radius;\n}\n\nh1, .h1 {\n    font-family: $font-2;\n    color: $font-color-h1;\n    font-size: $font-size-h1;\n    font-weight: $font-weight-h1;\n    line-height: $line-height-h1;\n    margin: $margin-h1;\n}\n\nh2, .h2 {\n    font-family: $font-2;\n    color: $font-color-h1;\n    font-size: $font-size-h2;\n    font-weight: $font-weight-h2;\n    line-height: $line-height-h2;\n    margin: $margin-h2;\n}\n\nh3, .h3 {\n    font-family: $font-2;\n    color: $font-color-h1;\n    font-size: $font-size-h3;\n    font-weight: $font-weight-h3;\n    line-height: $line-height-h3;\n    margin: $margin-h3;\n}\n\nh4, .h4 {\n    font-family: $font-2;\n    color: $font-color-h1;\n    font-size: $font-size-h4;\n    font-weight: $font-weight-h4;\n    line-height: $line-height-h4;\n    margin: $margin-h4;\n}","scss/Axelize/modules/_init.scss":"$screen-sizes: (\n    'xs': ('breakpoint' : 0,    'container-size': 90%),\n    's' : ('breakpoint' : 576px,    'container-size': 540px),\n    'm': ('breakpoint' : 768px,    'container-size': 720px),\n    'l': ('breakpoint' : 960px,    'container-size': 960px),\n     'xl': ('breakpoint' : 1200px,    'container-size': 1140px)\n );\n\n$border-radius-default: 5px;\n\n*{\n    box-sizing: border-box;\n}","scss/Axelize/modules/_colors.scss":"// COLORS\n\n$color-1: #18BFEF;\n$color-2: #F6F6F6;\n$color-3: #717981;\n$color-neutral: #4E403D;\n$color-text: #212931;\n\n$color-block-dim: 120px;\n\n//-----------------------\n// COLOR BLOCKS\n//-----------------------\n.color-block {\n    width: $color-block-dim;\n    height: $color-block-dim;\n    display: inline-block;\n    &.color-1{\n        background: $color-1;\n    }\n    &.color-2{\n        background: $color-2;\n    }\n    &.color-3{\n        background: $color-3;\n    }\n    &.color-neutral{\n        background: $color-neutral;\n    }\n    &.color-text{\n        background: $color-text;\n    }\n}","scss/Axelize/modules/_boxes.scss":"@use \"sass:map\";\n\n@each $size, $values in $screen-sizes {\n    @media screen and (min-width: map-get($values, 'breakpoint')){\n        .container{\n            width: 100%;\n            max-width: map-get($values,'container-size');\n            margin: auto;\n        }\n    }\n}\n\n//------------------------------\n// CONTENT ALIGN\n//------------------------------\n.text-center{\n    text-align: center;\n}\n\n.text-left{\n    text-align: left;\n}\n\n.text-right{\n    text-align: right;\n}\n\n.text-justify{\n    text-align: justify;\n}\n","scss/app.scss":"@import './Axelize/index.scss';\n\nbody{\n    background: radial-gradient(ellipse at top, $color-neutral, $color-text);\n}\n\n//----------------------------------\n// MAIN HEADER\n//----------------------------------\n.main-header{\n    margin: 5rem 0;\n    &--title1{\n        color: white;\n        border: 5px solid white;\n        display: inline-block;\n        padding: 1rem 1.75rem;\n    }\n}\n\n//----------------------------------\n// MAIN CONTENT\n//----------------------------------\n.main-content{\n    background: #fff;\n    padding: 2em;\n    border-radius: $border-radius-default;\n}\n\n//----------------------------------\n// MAIN FOOTER\n//----------------------------------\n.main-footer{\n\n}"},"lineCount":null}},"error":null,"hash":"e7883b3e7298594bc6407b9ebaa21043","cacheData":{"env":{}}}